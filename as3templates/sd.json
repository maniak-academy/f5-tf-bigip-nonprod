{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
     "class": "ADC",
     "schemaVersion": "3.0.0",
     "id": "urn:uuid:${UUID}",
     "label": "sd",
     "remark": "Simple HTTP application with RR pool",
     "sd": {
        "class": "Tenant",
        "A1": {
           "class": "Application",
           "template": "http",
           "serviceMain": {
              "class": "Service_HTTP",
              "virtualAddresses": [
                 "10.2.3.4"
              ],
              "pool": "sd_pool"
           },
           "sd_pool": {
              "class": "Pool",
              "monitors": [
                 "http"
              ],
              "members": [
                {
                  "servicePort": 80,
                  "addressDiscovery": "consul",
                  "updateInterval": 10,
                  "uri": "http://192.168.86.70:8500/v1/catalog/service/nginx",
                  "encodedToken": "base 64 encoded token",
                  "credentialUpdate": false
                },
                {
                  "enable": true,
                  "servicePort": 80,
                  "serverAddresses": [
                    "192.0.2.77"
                  ]
                }
              ]
           }
        }
     }
  }
}